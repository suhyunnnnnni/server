                });
            }

            function showPage(index) {
                if (index == 0) {
                    $('#page1').show();
                    $('#page2').hide();
                } else if (index == 1) {
                    $('#page1').hide();
                    $('#page2').show();
                }
            }

            function showAddPage() {
                mode = 'add';

                $('#page2Title').text('고객 추가');
                $('#nameInput').val('');
                $('#mobileInput').val('');

                showPage(1);
            }

            function searchItem() {
                search = $('#searchSelect').val();
                searchValue = $('#searchInput').val();
                console.log('search : ' + search + ', ' + searchValue);

