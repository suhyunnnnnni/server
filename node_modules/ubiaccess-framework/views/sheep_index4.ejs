            function saveItem() {
                if (mode == 'add') {
                    addInputItem();
                } else if (mode == 'modify') {
                    modifyInputItem();
                }

                showPage(0);
            }

            function addInputItem() {
                var name = $('#nameInput').val();
                var mobile = $('#mobileInput').val();
                
                requestAdd(name, mobile);

            }

